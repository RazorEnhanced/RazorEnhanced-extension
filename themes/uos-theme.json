{
    "$schema": "vscode://schemas/color-theme",
    "name": "UOSteam Theme",
    "colors": {
        "editor.foreground": "#FFFFFF",
        "editor.background": "#1E1E1E"
    },
    "tokenColors": [
        {
            "scope": "keyword.control.uos",
            "settings": {
                "foreground": "#5d5de0"
            }
        },
        {
            "scope": "keyword.operator.comparison.uos",
            "settings": {
                "foreground": "#90b6db"
            }
        },
        {
            "scope": "support.function.uos",
            "settings": {
                "foreground": "#569CD6"
            }
        },
        {
            "scope": "support.constant.uos",
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "scope": "comment.line.uos",
            "settings": {
                "foreground": "#ffd6ad"
            }
        },
        {
            "scope": "constant.language.uos",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": "constant.numeric.uos",
            "settings": {
                "foreground": "#c80f0f"
            }
        },
        {
            "scope": "constant.numeric.hexadecimal.uos",
            "settings": {
                "foreground": "#c80f0f"
            }
        },
        {
            "scope": "string.quoted.single",
            "settings": {
                "foreground": "#66ad3a"
            }
        },
        {
            "scope": "string.quoted.double",
            "settings": {
                "foreground": "#66ad3a"
            }
        }
    ]
}